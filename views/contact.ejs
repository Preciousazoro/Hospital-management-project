            <div class="col-2">
                <div class="contact-form">
                  <form id="contactForm">
                    <!-- First Name Input -->
                    <label for="first-name">First Name</label>
                    <input type="text" id="first-name" name="firstName" placeholder="Enter your First name" required>
                    
                    <!-- Email Input -->
                    <label for="email">Email Address</label>
                    <input type="email" id="email" name="email" placeholder="Enter your Email Address" required>
                    
                    <!-- Message Input -->
                    <label for="message">Question / Feedback</label>
                    <textarea id="message" name="feedback" rows="4" placeholder="Enter Your Question / Feedback" required></textarea>
                    
                    <!-- Hidden User Input -->
                    <input type="hidden" id="user" name="user" value="your-username"> <!-- Replace "your-username" with your actual username -->
              
                    <!-- Submit Button -->
                    <button type="submit">Send Message</button>
                  </form>
              
                  <!-- Success and Error Messages -->
                  <div id="responseMessage" style="display:none;"></div>
                </div>
              </div>
              
